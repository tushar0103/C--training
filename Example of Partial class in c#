namespace InheritanceExample
 
{

    internal partial class Example
    {
        protected int id;
        protected string name;
      
    }
   internal partial class Program : Example
    {
       public Program(int id, string name)
        {
            this.id=id;
            this.name=name;
        }
      
        static void Main(string[] args)
        {
            

        }
    }
   
